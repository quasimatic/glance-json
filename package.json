{
  "name": "glance-json",
  "version": "0.1.6",
  "description": "Use glance for referencing data in json",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && rm -rf dist && babel src -d lib && webpack",
    "pretest": "npm run build",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/quasimatic/glance.git"
  },
  "keywords": [
    "glance",
    "json",
    "reference",
    "query"
  ],
  "author": "Dan Gilkerson <dangilkerson@quasimatic.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/quasimatic/glance-json/issues"
  },
  "homepage": "https://github.com/quasimatic/glance-json#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "mocha": "^5.0.0",
    "webpack": "^3.11.0"
  },
  "babel": {
    "ignore": [
      "javascript/src/**/*-spec.js"
    ],
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "@arr/filter": "^1.0.0",
    "@arr/find": "^1.0.0",
    "@arr/foreach": "^1.0.0",
    "@arr/map": "^1.0.0",
    "@arr/reduce": "^1.0.0",
    "glance-parser": "0.0.7"
  }
}
